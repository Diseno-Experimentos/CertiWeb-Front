{
  "name": "selling-cars-quickly-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run --reporter dot",
    "test:unit": "vitest run --reporter verbose",
    "test:integration": "vitest run src/tests/integration/ --reporter verbose",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --reporter dot --coverage",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.vue",
    "lint:fix": "eslint . --ext .js,.vue --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docker:build": "docker build -t certiweb-front .",
    "docker:run": "docker run -p 8080:80 certiweb-front",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.4",
    "vercel": "^48.12.0",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.7",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@testing-library/vue": "^1.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^1.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "axios-mock-adapter": "^1.21.2",
    "eslint": "^8.50.0",
    "eslint-plugin-vue": "^9.14.0",
    "gh-pages": "^6.3.0",
    "jsdom": "^27.2.0",
    "prettier": "^2.8.8",
    "vite": "^6.3.5",
    "vitest": "^1.3.0"
  }
}
